@model _5H_Zaghini_Mattia.Models.Condominio
@{
    ViewData["Title"] = "Aggiungi Condominio";
}
<br>
<br>
<div class="text-center">
    <form class="m-2" method="POST" asp-action="AggiungiCond">
        <div class="form-group">
            <label asp-for="Nome">Nome condominio:</label>
            <input asp-for="Nome" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="Piani">Piani:</label>
            <input asp-for="Piani" class="form-control">
        </div>
        <br>
        <button type="submit" class="btn btn-primary">SALVA</button>
    </form>
</div>